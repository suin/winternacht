  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#A8A5A0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#5F9EA8"
      }
    },
    {
      "name": "Control keywords",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#5F9EA8"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#7A7672"
      }
    },
    {
      "name": "Operator keyword (new, delete, typeof, void, instanceof, in, of)",
      "scope": "keyword.operator.expression",
      "settings": {
        "foreground": "#5F9EA8"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#7A9E72"
      }
    },
    {
      "name": "String template / expression",
      "scope": [
        "string.template",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#6B9E8A"
      }
    },
    {
      "name": "Template string content",
      "scope": "string.template meta.template.expression",
      "settings": {
        "foreground": "#3A3835"
      }
    },
    {
      "name": "String regexp",
      "scope": [
        "string.regexp",
        "constant.other.character-class.regexp",
        "constant.character.escape.backslash.regexp"
      ],
      "settings": {
        "foreground": "#6B9E8A"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#C29E5E"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#C29E5E"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#8A7EB0"
      }
    },
    {
      "name": "Function parameters",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#6B8BBF"
      }
    },
    {
      "name": "Types and classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#B07888"
      }
    },
    {
      "name": "Interfaces",
      "scope": "entity.name.type.interface",
      "settings": {
        "foreground": "#B07888"
      }
    },
    {
      "name": "Enum members",
      "scope": [
        "variable.other.enummember",
        "constant.other.enum"
      ],
      "settings": {
        "foreground": "#C29E5E"
      }
    },
    {
      "name": "Type parameters / Generics",
      "scope": "entity.name.type.parameter",
      "settings": {
        "foreground": "#B07888"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#3A3835"
      }
    },
    {
      "name": "Variable constants",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#C29E5E"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#B08A60"
      }
    },
    {
      "name": "Language variables (this, self, super)",
      "scope": "variable.language",
      "settings": {
        "foreground": "#5F9EA8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#7A7672"
      }
    },
    {
      "name": "Punctuation - string delimiters",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#7A9E72"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#5F9EA8"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#B08A60",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS class names",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#B07888"
      }
    },
    {
      "name": "CSS IDs",
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#C29E5E"
      }
    },
    {
      "name": "CSS property names",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#B08A60"
      }
    },
    {
      "name": "CSS property values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#3A3835"
      }
    },
    {
      "name": "CSS units",
      "scope": "keyword.other.unit.css",
      "settings": {
        "foreground": "#C29E5E"
      }
    },
    {
      "name": "CSS pseudo-class / pseudo-element",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
